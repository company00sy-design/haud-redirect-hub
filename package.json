{
  "name": "haud-redirect-hub",
  "version": "1.0.0",
  "description": "URL redirect hub with analytics dashboard",
  "main": "dist/index.js",
  "scripts": {
    "build": "prisma generate && tsc",
    "start": "prisma db push && node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "deploy": "prisma generate && npm run build && prisma db push && ts-node prisma/seed.ts"
  },
  "keywords": ["redirect", "analytics", "express", "typescript"],
  "author": "",
  "license": "MIT",
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "cors": "^2.8.5",
    "helmet": "^7.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/cors": "^2.8.14",
    "prisma": "^5.4.2",
    "typescript": "^5.2.2",
    "ts-node-dev": "^2.0.0"
  }
}
