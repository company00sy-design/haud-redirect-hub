{
  "name": "haud-redirect-hub",
  "version": "1.0.0",
  "description": "URL redirect hub with analytics dashboard",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "deploy": "npm run build && npm run db:push && npm run db:generate && npm run db:seed",
    "postinstall": "prisma generate"
  },
  "keywords": ["redirect", "analytics", "express", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "cors": "^2.8.5",
    "helmet": "^7.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/cors": "^2.8.14",
    "prisma": "^5.4.2",
    "typescript": "^5.2.2",
    "ts-node-dev": "^2.0.0"
  }
}
